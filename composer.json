{
    "name": "deseco/repository",
    "description": "Easy repositories for laravel.",
    "keywords": ["repositories","pattern","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Żmudziński",
            "email": "jendrusha@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deseco\\Repositories\\RepositoriesServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
           "Deseco\\Repositories\\": "src/"
       },
        "files": [
            "src/helpers.php"
        ]
    }
}
